<%- include('../../layout/admin/header') %>
    <%- include('../../layout/admin/generalManagement') %>

        <div class="col-2 box  p-2 rounded-4  m-3 mt-4  text-center">
            <a class="nav-link text-white fs-5" href="/digiWorld/admin/catagoryManagement/createBrand">createBrand</a>
        </div>


        <% if(brands.length) {%>
            <div class="container-fluid">
                <table class="table table-dark table-hover  box">
                    <thead>
                        <tr>
                            <th scope="col">Number</th>
                            <th scope="col">Brand</th>
                            <th scope="col">No Of Models</th>
                           


                        </tr>
                    </thead>
                    <tbody>
                        <% let count=1 %>
                            <% for (let product of brands) { %>
                                <tr>
                                    <td>
                                        <%= count %>
                                    </td>
                                    <td>
                                        <%= product.brandName %>
                                    </td>
                                    <td>
                                        <%= product.noOfModels %>
                                    </td>

                                </tr>
                                <% count++ %>
                                    <% } %>
                    </tbody>
                </table>
            </div>
            <% } %>

                <%- include('../../layout/admin/footer') %>