<%- include('../../layout/admin/header') %>
    <%- include('../../layout/admin/generalManagement') %>
        <div class="col">
            <div class="bg-dark   p-2     rounded-1  m-1   text-start  position-absolute "
                style="top: 300px; left: 18px;">
                <a class="nav-link text-white fs-5" href="/createBrand">Create Brand</a>
            </div>


            <% if(brands.length) {%>
                <div class="container-fluid">
                    <table class="table table-dark table-hover  box">
                        <thead>
                            <tr>
                                <th scope="col">Number</th>
                                <th scope="col">Brand</th>
                                <th scope="col">No Of Models</th>



                            </tr>
                        </thead>
                        <tbody>
                            <% let count=1 %>
                                <% for (let product of brands) { %>
                                    <tr>
                                        <td>
                                            <%= count %>
                                        </td>
                                        <td>
                                            <%= product.brandName %>
                                        </td>
                                        <td>
                                            <%= product.noOfModels %>
                                        </td>

                                    </tr>
                                    <% count++ %>
                                        <% } %>
                        </tbody>
                    </table>
                </div>
                <% } %>
        </div>
        </div>
        </div>
        <%- include('../../layout/admin/footer') %>